{
  "name": "garba-styleguide",
  "version": "1.0.0-Development",
  "description": "A React.js styleguide to display all components contained in garba-ui",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass ./assets/scss/garba-styleguide.scss ./assets/css/garba-styleguide.css --output-style compressed",
    "sass:watch": "node-sass ./assets/scss/garba-styleguide.scss ./assets/css/garba-styleguide.css --output-style expanded --watch",
    "start-server": "node ./index.js",
    "start-dev": "npm run sass:watch & npm run start-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbarino-com/garba-styleguide.git"
  },
  "keywords": [
    "babel",
    "styleguide",
    "es6",
    "sass",
    "react",
    "workflow",
    "webpack",
    "sass",
    "UI framework"
  ],
  "author": [
    "Leonardo Pitelli <lpitelli@garbarino.com.ar>",
    "Esteban Calvi <ecalvi@garbarino.com.ar>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garbarino-com/garba-styleguide/issues"
  },
  "homepage": "https://github.com/garbarino-com/garba-styleguide#readme",
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "frontend-settings": "git+https://github.com/garbarino-com/frontend-settings.git"
  },
  "dependencies": {
    "express": "^4.15.2",
    "garba-ui": "^2.0.2-Alpha.2",
    "handlebars": "^4.0.8",
    "hbs": "^4.0.1",
    "node-sass": "^4.5.2"
  }
}
